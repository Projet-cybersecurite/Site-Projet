{
    "redirects": [
      {
        "source": "/(.*)",
        "destination": "https://projetcybersecurite-aykisws-projects.vercel.app/:path*",
        "permanent": true
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Strict-Transport-Security",
            "value": "max-age=63072000; includeSubDomains; preload"
          }
        ]
      },
      {
        "source": "/admin.html",
        "headers": [
          {
            "key": "X-Robots-Tag",
            "value": "noindex, nofollow"
          }
        ]
      }
    ]
  }  
